{
  "name": "we_app_deploy",
  "version": "1.0.0",
  "description": "This is an exercise to deploy full stack app",
  "main": "index.js",
  "scripts": {
    "start:dev": "node server",
    "server": "set NODE_ENV=dev && nodemon index.js",
    "prod": "NODE_ENV=production & nodemon app.js",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "start": "concurrently --kill-others-on-fail \"npm run server\" \"npm run start --prefix client\""
  },
  "engines": {
    "node": "14.16.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheCodingFreakj/we_app_deploy.git"
  },
  "author": "pallavi priyadarshini",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TheCodingFreakj/we_app_deploy/issues"
  },
  "homepage": "https://github.com/TheCodingFreakj/we_app_deploy#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^6.1.0",
    "cors": "^2.8.5",
    "dotenv": "^9.0.1",
    "express": "^4.17.1",
    "install": "^0.13.0",
    "mongoose": "^5.12.7",
    "nodemon": "^2.0.7"
  }
}
